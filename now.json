{
  "version": 2,
  "name": "portfolio",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/feed/(.*)",
      "dest": "/feed/$1"
    },
    {
      "src": "/cv.pdf",
      "status": 302,
      "headers": {
        "Location": "https://raphaelmarco.storage.googleapis.com/cv.pdf"
      }
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
